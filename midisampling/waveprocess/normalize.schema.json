{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Normalize",
    "description": "Structure of the normalize process configuration.",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "target_dBFS": {
            "type": "number",
            "description": "The target decibel level (dBFS)"
        }
    },
    "required": [
        "target_dBFS"
    ],
    "examples": [
        {
            "effects":[
                {
                    "index": 0,
                    "name": "normalize",
                    "params": {
                        "target_dBFS": -10.0
                    }
                }
            ]
        }
    ]
}
